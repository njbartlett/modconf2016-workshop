Bundle-Version: 1.0.0
Bundle-Description: An example of using Polymer Elements as Web Resources

Private-Package: org.example.webapp.bookstore

EnRoute-Application: org.example.webapp.bookstore

-includeresource: \
	{readme.md}, \
	{static=static}

-buildpath: \
	osgi.enroute.base.api,\
	osgi.enroute.polymer.iron.webresource,\
	osgi.enroute.polymer.app.webresource,\
	osgi.enroute.webcomponentsjs.webresource,\
	com.effectiveosgi.annotation
	
-testpath: \
	osgi.enroute.junit.wrapper;version=4.12
